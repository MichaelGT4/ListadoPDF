// Use a AJAX request to display the button to send the article as PDF to a customer email
window.Cip = window.Cip || {};
Cip.AgentFunctions = Cip.AgentFunctions || {};
Cip.AgentFunctions.show = $.ajax({
    data: {
        type: 470,
        no_cache: 1,
        plugin: 'tx_cipagentfunctions_show'
    },

    /**
     * Result of rendering the template 'showAgentLink'
     *
     * @param result string rendered HTML part to be included in page
     */
    success: function (result)
    {
        // HTML is a nested <DIV> element with content
        Cip.AgentFunctions.html = result.trim();

        // only if the current user is logged in and has the rights content is returned in the response
        if (Cip.AgentFunctions.html.length > 0)
        {
            // call all registered callbacks and hand them in the HTML and the callback to call after manipulation
            if (Cip.AgentFunctions.callbacks.has()) {
                Cip.AgentFunctions.callbacks.fire(Cip.AgentFunctions.html, Cip.AgentFunctions.resultCallback);
            }

            // insert it into the footer as last list element of the first list
            var targetContainer = $('div#content');
            targetContainer.append(Cip.AgentFunctions.html);

            // add the tooltip functionality
            targetContainer.find('.reveal-title-by-hover').each(function (index, element) {
                $(element).revealTitle();
            });

        }
    },

    error: function () {
        // fail silently - provide logging later on
    }
});

// setup registered callbacks - if the HTML should be manipulated before this hook can be used
Cip.AgentFunctions.callbacks = $.Callbacks();
Cip.AgentFunctions.registerCallback = function(callback) {
    Cip.AgentFunctions.callbacks.add(callback);
};

// provided callback to gather the manipulated HTML
Cip.AgentFunctions.resultCallback = function(html) {
    Cip.AgentFunctions.html = html;
};
window.Ciso = window.Ciso || {};
Ciso.ArticleToCustomer = Ciso.ArticleToCustomer || {};

Ciso.ArticleToCustomer.isUrlWithId = function(path) {
    return (path.search(/index\.php$/) !== -1 && document.location.search.indexOf('?id=') !== -1);
};

Ciso.ArticleToCustomer.getSpeakingUrlAndRedirect = function(queryString) {

    $.ajax({
        data: {
            type: 480,
            no_cache: 1
        },
        dataType : "json",

        // result is the speaking URL we can redirect to with the GET parameter
        success: function (result)
        {
            var speakingUrl = typeof(result) === 'object' ? result.speakingUrl : "";

            var loc = document.location;
            var path = loc.pathname.replace(/index\.php/, '');
            var baseUrl = loc.protocol + '//' + loc.host + path;
            loc.href = baseUrl + speakingUrl + queryString;
        },

        error: function () {
            // fail silently - provide logging later on
        }
    });

};


Ciso.ArticleToCustomer.redirect = function(queryString) {

    var baseUrl  = document.location.protocol + '//' + document.location.host + document.location.pathname;
    document.location.href = baseUrl + queryString;

};
// prefills the form for 'send mail to customer' with data from the parent document
// it's easier to do this on client-side - other values are pre-filled on server-side
window.document.addEventListener('DOMContentLoaded', function() {

    if (document.getElementById('sendarticlewithPDFlink'))
    {

        var pageName = window.top.document.querySelector('meta[name="page-name"]').getAttribute("content");
        document.getElementById('sendarticlewithPDFlink-pageName').value = pageName;

        var baseHref = window.top.document.querySelector('base').getAttribute('href');
        document.getElementById('sendarticlewithPDFlink-baseHref').value = baseHref;

        var pageId = window.top.document.querySelector('meta[name="page-id"]').getAttribute("content");
        document.getElementById('sendarticlewithPDFlink-pageId').value = pageId;

    }

});
window.Ciso = window.Ciso || {};
Ciso.ArticleToCustomer = Ciso.ArticleToCustomer || {};

Ciso.ArticleToCustomer.generatePdf = function(ev) {

    ev.preventDefault();
    ev.stopPropagation();

    this.style.display = 'none';
    document.getElementById('pdfDownloadLoadingIndicator').style.display = 'inline-block';
    Ciso.ArticleToCustomer.intervalId = window.setInterval(Ciso.ArticleToCustomer.hideLoadingIndicator.bind(this), 1000);

    var queryString = '?downloadArticleAsPdf=1';
    var path = document.location.pathname;
    if (Ciso.ArticleToCustomer.isUrlWithId(path)) {
        Ciso.ArticleToCustomer.getSpeakingUrlAndRedirect(queryString);
    } else {
        Ciso.ArticleToCustomer.redirect(queryString);
    }

};

Ciso.ArticleToCustomer.hideLoadingIndicator = function() {

    if (document.cookie.indexOf('PdfDownloadComplete') !== -1) {

        document.getElementById('pdfDownloadLoadingIndicator').style.display = 'none';
        this.style.display = '';
        window.clearInterval(Ciso.ArticleToCustomer.intervalId);

    }

};

document.addEventListener('DOMContentLoaded', function(e) {

    document.querySelectorAll('a.generate-pdf-icon').forEach(function (element) {
        element.addEventListener('click', window.Ciso.ArticleToCustomer.generatePdf);
    });

});

window.Ciso = window.Ciso || {};
Ciso.ArticleToCustomer = Ciso.ArticleToCustomer || {};

/**
 * Manipulate the HTML if necessary before it is displayed
 *
 * @param html string provided HTML as string
 * @param callback function callback to execute to return the value
 */
Ciso.ArticleToCustomer.setupAppearance = function(html, callback)
{
    var result = html;

    // disable the link if it is displayed on any non-article page and add a additional hint for the agents
    var container = $('#content');
    if (!container.hasClass('knowledge') && !container.hasClass('marketing') && !container.hasClass('glossary'))
    {
        var htmlObject = $(html);
        var mailLink = htmlObject.find('#pdfMailingText');
        mailLink.addClass('disabled').removeClass('oao-pi-open-in-overlay');
        mailLink.attr('href', null);

        var title = mailLink.attr('title') + mailLink.data('labelNotavailable');
        mailLink.attr('title', title);

        // create a temporary object to transform it back into a string
        result = $('<div>').append(htmlObject).clone().html();
    }

    // use a callback for returning the HTML to be sure it is synchronous
    callback.call(window, result);
};

// register callback to manipulate the HTML
if (window.Cip && Cip.AgentFunctions &&
    Cip.AgentFunctions.registerCallback &&
    jQuery.isFunction(Cip.AgentFunctions.registerCallback)) {

    Cip.AgentFunctions.registerCallback(Ciso.ArticleToCustomer.setupAppearance);
}
// creating namespace if not existing
window.CisoLog = window.CisoLog || {};

$(window).ready(function() {
    CisoLog.sendTrailLog();
    CisoLog.sendUtmLog();
    CisoLog.sendTrackLog();
});

CisoLog.sendTrailLog = function()
{
    $.ajax({
        data: {
            type: 3601,
            no_cache: 1,
            tx_cisotraillog: {
                params: window.document.location.search.substring(1),
                referrer: window.encodeURIComponent(window.document.referrer)
            }
        },
        success: function (result) {
            try {
                result = (JSON.parse(result));
            } catch (err) {

            }

            if(result.url) {
                window.location.href = result.url;
            }
        },
        error: function(e) {
        }
    });
};

CisoLog.sendUtmLog = function()
{
    var params = document.location.search.substring(1);
    var parts = params.split('&');

    var found = false;
    var utm_log = {
        utm_source: '',
        utm_campaign: '',
        utm_medium: '',
        utm_term: '',
        utm_content: ''
    };
    for (var part in parts) {
        var value = parts[part];

        if (value.indexOf("utm_") !== -1) {
            var utmParts = value.split("=");
            utm_log[utmParts[0]] = utmParts[1];
            found = true;
        }
    }

    if (found) {
        $.ajax({
            data: {
                type: 3602,
                no_cache: 1,
                tx_utmlog: utm_log
            },
            success: function () {
            },
            error: function(e) {
            }
        });
    }
};

CisoLog.sendTrackLog = function()
{
    // get all tracking links and intercept them with a event handler
    // differentiate between internal and external links
    var trackLinks = $('a[data-tracklink]');
    trackLinks.each(function() {

        if ($(this).attr('target') === '_blank') {

            $(this).click(function() {
                CisoLog.trackLink(this, false);
            });

        } else {

            $(this).click(function(e) {
                e.stopPropagation();
                e.preventDefault();
                CisoLog.trackLink(this, true);
            });

        }

    });

};

CisoLog.trackLink = function(element, redirect) {
    var link = $(element);
    var href = link.attr('href');
    var desc = link.data('tracklinkDesc');

    // send the data and redirect to the requested URL if it's in in our own document (same window)
    $.ajax({
        data: {
            type: 3603,
            no_cache: 1,
            tx_tracklog: {
                trackLinkHref: href,
                trackLinkDesc: desc
            }
        }
    }).always(function(){
        if (redirect === true) {
            document.location.href = href;
        }
    });
};
